{
    "name": "magabriel/trefoil",
    "description": "Book publishing",
    "keywords": [
        "book",
        "publish",
        "ebook",
        "e-book",
        "documentation"
    ],
    "homepage": "http://magabriel.github.io/trefoil-doc",
    "license": "MIT",
    "authors": [
        {
            "name": "Miguel Angel Gabriel",
            "email": "magabriel@gmail.com",
            "homepage": "https://github.com/magabriel/"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:magabriel/easybook.git"
        }
    ],
    "require": {
        "php": ">=7.3",
        "easybook/easybook": "dev-master",
        "symfony/dom-crawler": "^2.3",
        "symfony/css-selector": "^2.3",
        "michelf/php-smartypants": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "autoload": {
        "psr-0": {
            "Trefoil": "src/"
        }
    }
}
